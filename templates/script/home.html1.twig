{% extends 'base.html.twig' %}
{% block body %}
        <table id="table_id" class="display" style="width:100%">
        <thead>
        <tr>
            <th scope="col">Mitarbeiter</th>
            <th scope="col">Project Name</th>
            <th scope="col">Projects Beschreibung</th>
            <th scope="col">Aufgaben name</th>
            <th scope="col">Aufgaben Beschreibung</th>
            <th scope="col">Kommentare</th>
            <th scope="col">Zeiten</th>
        </tr>
        </thead>
        <tbody>
        {% for tes in myJSON %}
        <tr class="table-active">
            <td> {{ tes.nachname}}</td>
            <td> {{ tes.project_name}}</td>
            <td> {{ tes.description}}</td>
            <td> {{ tes.name}}</td>
            <td> {{ tes.Aufgaben}}</td>
            <td> {{ tes.internal_commentary}}</td>
            <td> {{ tes.Zeiten}}</td>
        </tr>
        </tbody>
        {% endfor %}

    </table>
{% endblock %}

