{% extends 'base.html.twig' %}


{% block body %}
    <div class="container">
        <table class="table table-hover mt-5">
            <thead>
            <tr class="table-primary  text-white"  >
                <th scope="col">Projekte</th>
                <th scope="col">GesamtZeite</th>

            </tr>
            </thead>
            <tbody>

            {% for project in projectHour %}

                <tr>
                    <td> {{ project.project_name}}</td>
                    <td> {{ project.GesamtStunden|date('H:i')}}</td>
                </tr>

            {% endfor %}
            </tbody>
        </table>
    </div>
{% endblock %}
